import("../build_webui_tests.gni")

assert(!is_android)

build_webui_tests("build") {
  files = [
    "tab_url_item_grid_test.ts",
    "url_item_test.ts",
  ]

  ts_path_mappings =
      [ "chrome://tab-group-home/*|" +
        rebase_path(
            "$root_gen_dir/chrome/browser/resources/tab_group_home/tsc/*",
            target_gen_dir) ]

  ts_deps = [
    "//chrome/browser/resources/tab_group_home:build_ts",
    "//ui/webui/resources/js:build_ts",
  ]
}
